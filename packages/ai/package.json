    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/bull": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@storige/types": "workspace:*",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "bull": "^4.0.0",
    "class-transformer": "^0.5.0",
    "class-validator": "^0.14.0",
    "lucide-react": "^0.400.0",
    "react": "^18.0.0",
    "tsup": "^8.0.0",
    "typeorm": "^0.3.0",
