    "lucide-react": "^0.400.0"
  },
  "peerDependenciesMeta": {
    "@nestjs/common": {
      "optional": true
    },
    "@nestjs/core": {
      "optional": true
    },
    "@nestjs/bull": {
      "optional": true
    },
    "typeorm": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "lucide-react": {
      "optional": true
    }
  },
